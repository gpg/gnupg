#, c-format
msgid "SETERROR %s (try %d of %d)"
msgstr "SETERROR %s (第 %d 次嘗試, 最多 %d 次)"

msgid "Repeat:"
msgstr "重複:"

msgid "PIN too long"
msgstr "個人識別碼 (PIN) 太長"

